    "smart-contracts",
    "decentralized",
    "DApps",
    "web3",
    "interchain"
  ],
  "devDependencies": {
    "@types/js-yaml": "^4.0.9"
  },
  "dependencies": {
    "@starship-ci/client": "^1.4.0",
    "chalk": "^4.1.0",
    "deepmerge": "^4.3.1",
    "inquirerer": "^1.9.0",
    "js-yaml": "^4.1.0",
